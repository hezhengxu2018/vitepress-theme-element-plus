{
  "name": "vitepress-theme-element-plus",
  "type": "module",
  "version": "0.0.0",
  "description": "A VitePress theme for Element Plus",
  "author": "Hezhengxu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hezhengxu/vitepress-theme-element-plus"
  },
  "keywords": [
    "vitepress",
    "theme",
    "vue",
    "documentation"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./index.ts"
    },
    "./node": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./styles/*": "./styles/*"
  },
  "module": "./index.ts",
  "types": "./types/index.d.ts",
  "style": "./index.scss",
  "files": [
    "config",
    "index.ts",
    "src",
    "types"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "prepublishOnly": "pnpm run build",
    "release": "npm publish --access public"
  },
  "peerDependencies": {
    "vitepress": "2.0.0-alpha.12"
  },
  "devDependencies": {
    "@iconify/vue": "^4.2.1",
    "@types/glob": "^9.0.0",
    "@types/markdown-it": "^14.1.0",
    "@types/node": "^22.17.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vueuse/core": "^11.3.0",
    "element-plus": "^2.11.7",
    "es-toolkit": "^1.14.0",
    "rimraf": "^6.0.1",
    "sass-embedded": "^1.80.3",
    "tsdown": "0.16.0",
    "typescript": "^5.9.2",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "2.0.0-alpha.12",
    "vue": "^3.5.18",
    "vue-tsc": "^3.0.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
